    <converters:FilterHeaderColumnName x:Key="HeaderConverter"/>
    <Style x:Key="FilteringColumnStyle" TargetType="{x:Type GridViewColumnHeader}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type GridViewColumnHeader}">
                    <Grid SnapsToDevicePixels="True">
                        <Border x:Name="HeaderBorder" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0,1" Background="{TemplateBinding Background}">
                            <DockPanel>
                                <Border Name="CheckColumnBorder" Tag="{TemplateBinding Content}" BorderThickness="1" DockPanel.Dock="Right" VerticalAlignment="Center">
                                    <CheckBox Name="CheckColumn" Tag="{Binding ElementName=CheckColumnBorder}" IsChecked="{Binding IsCheckedCurrentColumn}" Command="{Binding SetCurrentFilterCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Self}}">
                                        <CheckBox.Template>
                                            <ControlTemplate TargetType="CheckBox">
                                                <Image x:Name="ImageFilter" Source="/Style/Image/Empty Filter-24.png" Width="14" Height="14"/>
                                            </ControlTemplate>
                                        </CheckBox.Template>
                                    </CheckBox>
                                </Border>
                                <Popup x:Name="FilteringPopup" Tag="{Binding ElementName=CheckColumnBorder}" IsOpen="{Binding Path=IsChecked, ElementName=CheckColumn}" PlacementTarget="{Binding ElementName=CheckColumn}">
                                    <Border BorderThickness="1" BorderBrush="Gray">
                                        <Grid Width="200" Height="300" Background="WhiteSmoke">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="10*"/>
                                                <RowDefinition Height="80*"/>
                                                <RowDefinition Height="10*"/>
                                            </Grid.RowDefinitions>
                                            <Grid Grid.Row="0">
                                                <StackPanel  Orientation="Horizontal">
                                                    <TextBox Text="{Binding CurrentColumn.SearchText}" Width="150" Height="30"/>
                                                    <Button Command="{Binding SearchFromCurrentColumnCommand}" CommandParameter="{Binding ElementName=FilteringPopup}">искать</Button>
                                                </StackPanel>
                                            </Grid>
                                            <Grid Grid.Row="1">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="10*"/>
                                                    <RowDefinition Height="90*"/>
                                                </Grid.RowDefinitions>
                                                <CheckBox Grid.Row="0" Content="выбрать все" FontWeight="Bold" Command="{Binding CheckAllItemCommand}" CommandParameter="{Binding Path=IsChecked, RelativeSource={RelativeSource Self}}"></CheckBox>
                                                <ListBox Grid.Row="1"  ItemsSource="{Binding CurrentColumn.List}">
                                                    <ListBox.ItemTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <CheckBox IsChecked="{Binding IsChecked}"></CheckBox>
                                                                <TextBlock Text="{Binding Text}"></TextBlock>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </ListBox.ItemTemplate>
                                                    <ListBox.ItemContainerStyle>
                                                        <Style TargetType="ListBoxItem">
                                                            <Style.Triggers>
                                                                <DataTrigger Binding="{Binding IsVisible}" Value="False">
                                                                    <Setter Property="Visibility" Value="Collapsed"/>
                                                                </DataTrigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </ListBox.ItemContainerStyle>
                                                </ListBox>
                                            </Grid>
                                            <Grid Grid.Row="2">
                                                <StackPanel  Orientation="Horizontal">
                                                    <Button Command="{Binding ClearFilterCommand}" CommandParameter="{Binding ElementName=FilteringPopup}">сбросить</Button>
                                                    <Button Command="{Binding FilteringCommand}" CommandParameter="{Binding ElementName=FilteringPopup}">приминить</Button>
                                                    <Button Command="{Binding ClosePopupCommand}" CommandParameter="{Binding ElementName=FilteringPopup}">закрыть</Button>
                                                </StackPanel>
                                            </Grid>
                                        </Grid>
                                    </Border>
                                </Popup>
                                <Border Padding="{TemplateBinding Padding}">
                                    <ContentPresenter x:Name="HeaderContent"  Content="{TemplateBinding Content,Converter={StaticResource ResourceKey=HeaderConverter}}" ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}" ContentStringFormat="{TemplateBinding ContentStringFormat}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="0,0,0,1" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                                </Border>
                            </DockPanel>
                        </Border>
                        <Border x:Name="HeaderHoverBorder" BorderThickness="1,0,1,1" Margin="1,1,0,0"/>
                        <Border x:Name="HeaderPressBorder" BorderThickness="1,1,1,0" Margin="1,0,0,1"/>
                        <Canvas>
                            <Thumb x:Name="PART_HeaderGripper">
                                <Thumb.Style>
                                    <Style TargetType="{x:Type Thumb}">
                                        <Setter Property="Canvas.Right" Value="-9"/>
                                        <Setter Property="Width" Value="10"/>
                                        <Setter Property="Height" Value="{Binding ActualHeight, RelativeSource={RelativeSource TemplatedParent}}"/>
                                        <Setter Property="Padding" Value="0"/>
                                        <Setter Property="Background">
                                            <Setter.Value>
                                                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                                    <GradientStop Color="#FFF2F2F2" Offset="0"/>
                                                    <GradientStop Color="#FFD5D5D5" Offset="1"/>
                                                </LinearGradientBrush>
                                            </Setter.Value>
                                        </Setter>
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="{x:Type Thumb}">
                                                    <Border Background="Transparent" Padding="{TemplateBinding Padding}">
                                                        <Rectangle Fill="{TemplateBinding Background}" HorizontalAlignment="Center" Width="1"/>
                                                    </Border>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                    </Style>
                                </Thumb.Style>
                            </Thumb>
                        </Canvas>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" TargetName="HeaderBorder">
                                <Setter.Value>
                                    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                        <GradientStop Color="#FFBDEDFF" Offset="0"/>
                                        <GradientStop Color="#FFB7E7FB" Offset="1"/>
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="BorderBrush" TargetName="HeaderHoverBorder" Value="#FF88CBEB"/>
                            <Setter Property="Background" TargetName="PART_HeaderGripper" Value="Transparent"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Background" TargetName="HeaderBorder">
                                <Setter.Value>
                                    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                        <GradientStop Color="#FF8DD6F7" Offset="0"/>
                                        <GradientStop Color="#FF8AD1F5" Offset="1"/>
                                    </LinearGradientBrush>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="BorderBrush" TargetName="HeaderHoverBorder" Value="#FF95DAF9"/>
                            <Setter Property="BorderBrush" TargetName="HeaderPressBorder" Value="#FF7A9EB1"/>
                            <Setter Property="Visibility" TargetName="PART_HeaderGripper" Value="Hidden"/>
                            <Setter Property="Margin" TargetName="HeaderContent" Value="1,1,0,0"/>
                        </Trigger>
                        <Trigger Property="Height" Value="Auto">
                            <Setter Property="MinHeight" Value="20"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <!--<Setter Property="Background" Value="Gray"></Setter>-->
    </Style>

    <Style x:Key="ListViewStyle" TargetType="ListView">
        <Setter Property="ItemContainerStyle">
            <Setter.Value>
                <Style TargetType="ListViewItem">
                    <Style.Triggers>
                        <Trigger Property="ItemsControl.AlternationIndex" Value="0">
                            <Setter Property="Background" Value="White"/>
                        </Trigger>
                        <Trigger Property="ItemsControl.AlternationIndex" Value="1">
                            <Setter Property="Background" Value="WhiteSmoke"/>
                        </Trigger>
                    </Style.Triggers>
                    <Setter Property="Height" Value="Auto"></Setter>
                    <Setter Property="FontSize" Value="12"></Setter>
                    <Setter Property="Foreground" Value="DarkSlateGray"></Setter>
                    <Setter Property="HorizontalContentAlignment" Value="Stretch" />
                    <Setter Property="VerticalContentAlignment" Value="Stretch" />
                </Style>
            </Setter.Value>
        </Setter>
        <Setter Property="AlternationCount" Value="2"></Setter>
    </Style>
    <Style x:Key="TabItemPagesStyle" TargetType="TabItem">
        <Style.Triggers>
            <DataTrigger Binding="{Binding Path=Type}" Value="{x:Type cashTransaction:CashTransactionPageViewModel}">
                <Setter Property="Header" Value="{Binding Path=Name}"></Setter>
                <Setter Property="Content">
                    <Setter.Value>
                        <!--<Grid>-->
                            <ListView  Style="{StaticResource ListViewStyle}" SelectionMode="Single" ItemsSource="{Binding FilteringList}" SelectedItem="{Binding CurrentItem}">
                                <ListView.InputBindings>
                                    <KeyBinding Key="Enter" Command="{Binding OpenListItem}"/>
                                </ListView.InputBindings>
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="MouseDoubleClick">
                                        <i:InvokeCommandAction Command="{Binding OpenListItem}"/>
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                                <ListView.View>
                                    <GridView>
                                        <GridViewColumn x:Name="Id"  Width="0" Header="id"  DisplayMemberBinding="{Binding Id}"/>
                                        <GridViewColumn x:Name="Date" Width="Auto" Header="ДАТА|Date" DisplayMemberBinding="{Binding Date}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"/>
                                        <GridViewColumn x:Name="Number" Width="Auto" Header="№|Number" DisplayMemberBinding="{Binding Number}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                         <GridViewColumn x:Name="NumberProxy"  Width="Auto" Header="ДОВЕРЕННОСТЬ|NumberProxy" DisplayMemberBinding="{Binding NumberProxy}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                        <GridViewColumn x:Name="SellerFullName"  Width="Auto" Header="ПРОДАВЕЦ|SellerFullName" DisplayMemberBinding="{Binding SellerFullName}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                    <GridViewColumn x:Name="BuyerFullName"  Width="Auto" Header="ПОКУПАТЕЛЬ|BuyerFullName" DisplayMemberBinding="{Binding BuyerFullName}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                    <GridViewColumn x:Name="TrancportFullName"  Width="Auto" Header="МОДЕЛЬ, МАРКА ТС|TrancportFullName" DisplayMemberBinding="{Binding TrancportFullName}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                    <GridViewColumn Width="Auto">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Visibility="{Binding VisibilityOpenDocumentSeller}">
                                                        <Hyperlink  Command="{Binding OpenDocumentSellerCommand}">ОТКРЫТЬ</Hyperlink>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                            <GridViewColumn.Header>
                                                <TextBlock TextAlignment="Center" Padding="0 3">
                                                    <TextBlock.Inlines>
                                                        <Run>ПАСПОРТ</Run>
                                                        <LineBreak />
                                                        <Run>ПРОДОВЦА</Run>
                                                    </TextBlock.Inlines>
                                                </TextBlock>
                                            </GridViewColumn.Header>
                                        </GridViewColumn>
                                    <GridViewColumn Width="Auto">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Visibility="{Binding VisibilityOpenDocumentBuyer}">
                                                        <Hyperlink  Command="{Binding OpenDocumentBuyerCommand}">ОТКРЫТЬ</Hyperlink>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                            <GridViewColumn.Header>
                                                <TextBlock TextAlignment="Center" Padding="0 3">
                                                    <TextBlock.Inlines>
                                                        <Run>ПАСПОРТ</Run>
                                                        <LineBreak />
                                                        <Run>ПОКУПАТЕЛЯ</Run>
                                                    </TextBlock.Inlines>
                                                </TextBlock>
                                            </GridViewColumn.Header>
                                        </GridViewColumn>
                                    <GridViewColumn Header="ПТС"  Width="Auto">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Visibility="{Binding VisibilityOpenPts}">
                                                        <Hyperlink  Command="{Binding OpenPtsCommand}">ОТКРЫТЬ</Hyperlink>
                                                    </TextBlock>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                        </GridViewColumn>
                                    </GridView>
                                </ListView.View>
                            </ListView>
                        <!--</Grid>-->
                        <!--</StackPanel>-->
                    </Setter.Value>
                </Setter>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=Type}" Value="{x:Type reportOfClient:ClientReportPageViewModel}">
                <Setter Property="Header" Value="{Binding Path=Name}"></Setter>
                <Setter Property="Content">
                    <Setter.Value>
                        <Grid>
                            <ListView x:Name="ClientReportListView"  Style="{StaticResource ListViewStyle}" SelectionMode="Single" ItemsSource="{Binding FilteringList}" SelectedItem="{Binding CurrentItem}">
                                <ListView.InputBindings>
                                    <KeyBinding Key="Enter" Command="{Binding OpenListItem}"/>
                                </ListView.InputBindings>
                                <i:Interaction.Triggers>
                                    <i:EventTrigger EventName="MouseDoubleClick">
                                        <i:InvokeCommandAction Command="{Binding OpenListItem}"/>
                                    </i:EventTrigger>
                                </i:Interaction.Triggers>
                                <ListView.View>
                                    <GridView>
                                        <GridView.ColumnHeaderContainerStyle>
                                            <Style TargetType="GridViewColumnHeader">
                                                <Setter Property="Height"  Value="Auto"  />
                                            </Style>
                                        </GridView.ColumnHeaderContainerStyle>
                                        <GridViewColumn Width="0" Header="id"  DisplayMemberBinding="{Binding Id}"/>
                                        <GridViewColumn x:Name="DateReportClient" Width="Auto" Header="ДАТА|DateReportClient" DisplayMemberBinding="{Binding DateReportClient}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"/>
                                        <GridViewColumn x:Name="FullNameReportClient" Width="Auto" Header="Ф.И.О. ЗАЕМЩИКА|FullNameReportClient" DisplayMemberBinding="{Binding FullNameReportClient}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                        <GridViewColumn x:Name="TelefonReportClient" Width="Auto" Header="ТЕЛЕФОН|TelefonReportClient" DisplayMemberBinding="{Binding TelefonReportClient}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                        <GridViewColumn x:Name="TrancportNameReportClient"  Width="Auto" Header="ДАННЫЕ ТС|TrancportNameReportClient" DisplayMemberBinding="{Binding TrancportNameReportClient}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                        <GridViewColumn x:Name="PriceTrancportReportClient"  Width="Auto" Header="СТОИМОСТЬ ТС|PriceTrancportReportClient" DisplayMemberBinding="{Binding PriceTrancportReportClient}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                        <GridViewColumn x:Name="TotalContributionReportClient"  Width="Auto" Header="ОБЩИЙ ВЗНОС|TotalContributionReportClient" DisplayMemberBinding="{Binding TotalContributionReportClient}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"></GridViewColumn>
                                        <GridViewColumn x:Name="ProgrammCreditReportClient" Width="Auto" Header="ПРОГРАММА КРЕДИТОВАНИЯ|ProgrammCreditReportClient" DisplayMemberBinding="{Binding ProgrammCreditReportClient}" HeaderContainerStyle="{StaticResource ResourceKey=FilteringColumnStyle}"/>
                                        <GridViewColumn x:Name="BankStatusesReportClient" Width="Auto">
                                            <GridViewColumn.CellTemplate>
                                                <DataTemplate>
                                                    <ItemsControl  ItemsSource="{Binding BankStatusesReportClient}">
                                                        <ItemsControl.ItemsPanel>
                                                            <ItemsPanelTemplate>
                                                                <StackPanel Orientation="Horizontal"/>
                                                            </ItemsPanelTemplate>
                                                        </ItemsControl.ItemsPanel>
                                                        <ItemsControl.ItemTemplate>
                                                            <DataTemplate>
                                                                <Border BorderThickness="0 0 1 0" BorderBrush="Black" Width="20">
                                                                    <TextBlock Text="{Binding}" TextAlignment="Center"/>
                                                                </Border>
                                                            </DataTemplate>
                                                        </ItemsControl.ItemTemplate>
                                                    </ItemsControl>
                                                </DataTemplate>
                                            </GridViewColumn.CellTemplate>
                                            <GridViewColumn.HeaderTemplate>
                                                <DataTemplate>
                                                    <ItemsControl DataContext="{Binding Path=DataContext,Source={x:Reference Name=ClientReportListView}}" ItemsSource="{Binding BanksColumnName}">
                                                        <ItemsControl.ItemsPanel>
                                                            <ItemsPanelTemplate>
                                                                <StackPanel Orientation="Horizontal"/>
                                                            </ItemsPanelTemplate>
                                                        </ItemsControl.ItemsPanel>
                                                        <ItemsControl.ItemTemplate>
                                                            <DataTemplate>
                                                                <Border BorderThickness="0 0 1 0" BorderBrush="Black" Width="20">
                                                                    <TextBlock Text="{Binding}">
                                                                        <TextBlock.LayoutTransform>
                                                                            <TransformGroup>
                                                                                <RotateTransform Angle="-90" />
                                                                            </TransformGroup>
                                                                        </TextBlock.LayoutTransform>
                                                                    </TextBlock>
                                                                </Border>
                                                            </DataTemplate>
                                                        </ItemsControl.ItemTemplate>
                                                    </ItemsControl>
                                                </DataTemplate>
                                            </GridViewColumn.HeaderTemplate>
                                        </GridViewColumn>
                                        <GridViewColumn x:Name="ClientStatusReportClient"  Width="Auto" Header="СТАТУС КЛИЕНТА|ClientStatusReportClient" DisplayMemberBinding="{Binding ClientStatusReportClient}">
                                            <GridViewColumn.HeaderContainerStyle>
                                                <Style TargetType="GridViewColumnHeader">
                                                    <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type GridViewColumnHeader}">
                    <Grid SnapsToDevicePixels="True">
                        <Border x:Name="HeaderBorder" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0,1" Background="{TemplateBinding Background}">
                            <DockPanel>
                                <Border Name="CheckColumnBorder" BorderBrush="{Binding KASTIL_BRASH_FOR_CLIENT_STATUS}" Tag="{TemplateBinding Content}" BorderThickness="1" DockPanel.Dock="Right" VerticalAlignment="Center">
                                    <CheckBox Name="CheckColumn" Tag="{Binding ElementName=CheckColumnBorder}" IsChecked="{Binding IsCheckedCurrentColumn}" Command="{Binding SetCurrentFilterCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Self}}">
                                        <CheckBox.Template>
                                            <ControlTemplate TargetType="CheckBox">
                                                <Image x:Name="ImageFilter" Source="/Style/Image/Empty Filter-24.png" Width="14" Height="14"/>
                                            </ControlTemplate>
                                        </CheckBox.Template>
                                    </CheckBox>
                                </Border>
                                <Popup x:Name="FilteringPopup" Tag="{Binding ElementName=CheckColumnBorder}" IsOpen="{Binding Path=IsChecked, ElementName=CheckColumn}" PlacementTarget="{Binding ElementName=CheckColumn}">
                                    <Border BorderThickness="1" BorderBrush="Gray">
                                        <Grid Width="200" Height="300" Background="WhiteSmoke">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="10*"/>
                                                <RowDefinition Height="80*"/>
                                                <RowDefinition Height="10*"/>
                                            </Grid.RowDefinitions>
                                            <Grid Grid.Row="0">
                                                <StackPanel  Orientation="Horizontal">
