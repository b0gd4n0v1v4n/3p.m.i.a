            <TextBox x:Name="textBox_Copy14" Text="{Binding CardTrancport.Rubber,Mode=TwoWay}"  HorizontalAlignment="Left" Height="23" Margin="139,209,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="382"/>
